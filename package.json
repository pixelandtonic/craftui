{
  "name": "@pixelandtonic/craftui",
  "version": "0.3.6",
  "main": "dist/craftui.js",
  "unpkg": "dist/craftui.iife.min.js",
  "module": "dist/craftui.esm.min.js",
  "scripts": {
    "build": "bili --config bili.config.js",
    "watch": "bili --config bili.config.js --watch",
    "serve:docs": "VUE_CLI_SERVICE_CONFIG_PATH=$PWD/docs/vue.config.js start-storybook -p 6006 -c docs/config",
    "build:docs": "VUE_CLI_SERVICE_CONFIG_PATH=$PWD/docs/vue.config.js build-storybook -c docs/config -o docs/dist"
  },
  "dependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "vue": "^2.6.7",
    "vue-the-mask": "^0.11.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "5.1.9",
    "@storybook/addon-knobs": "^5.1.9",
    "@storybook/addon-links": "5.1.9",
    "@storybook/addon-notes": "^5.1.9",
    "@storybook/addon-storysource": "^5.1.9",
    "@storybook/addon-viewport": "^5.1.9",
    "@storybook/theming": "^5.1.9",
    "@storybook/vue": "^5.1.9",
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "babel-preset-vue": "^2.0.2",
    "bili": "^4.8.0",
    "node-sass": "^4.11.0",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-vue": "^5.0.0",
    "sass-loader": "^7.1.0",
    "tailwindcss": "^1.0.4",
    "vue-template-compiler": "^2.6.7"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 5%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
