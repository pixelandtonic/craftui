{
  "name": "@pixelandtonic/craftui",
  "version": "0.2.0",
  "private": true,
  "main": "dist/craftui.js",
  "unpkg": "dist/craftui.min.js",
  "module": "dist/craftui.es.js",
  "scripts": {
    "build": "bili --config bili.config.js",
    "serve:docs": "VUE_CLI_SERVICE_CONFIG_PATH=$PWD/docs/vue.config.js vue-cli-service storybook:serve -p 6006 -c docs/config",
    "build:docs": "VUE_CLI_SERVICE_CONFIG_PATH=$PWD/docs/vue.config.js vue-cli-service storybook:build -c docs/config -o docs/dist"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/vue-fontawesome": "^0.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "inputmask": "^4.0.0",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-vue": "^4.6.1",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@storybook/addon-actions": "4.0.0-alpha.20",
    "@storybook/addon-links": "4.0.0-alpha.20",
    "@storybook/addon-options": "^4.1.11",
    "@storybook/addon-storysource": "^4.1.11",
    "@storybook/addon-viewport": "^4.1.11",
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "bili": "^3.4.2",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "tailwindcss": "^0.7.3",
    "vue-cli-plugin-storybook": "^0.5.0",
    "vue-template-compiler": "^2.5.22"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 5%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
